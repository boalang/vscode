{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",

	"compilerOptions": {
		"target": "es2019",
		"lib": [
			"es2020.string",
            "es2019"
        ],
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "out",
		"sourceMap": true,
		"plugins": [{ "name": "typescript-tslint-plugin" }]
	}
}